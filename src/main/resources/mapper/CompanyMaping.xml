<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xxxx.hrhealthcheck.dao.CompanyDao" >
    <insert id="companyInfo" parameterType="com.xxxx.hrhealthcheck.bean.Company">
        insert into companyinfo (companyName,businessSector,employeeNumber,companyLocation,contactName,contactJobTitle,contactEmail,contactTelephone)
        values (#{companyName}, #{businessSector}, #{employeeNumber}, #{companyLocation}, #{contactName}, #{contactJobTitle}, #{contactEmail}, #{contactTelephone});
    </insert>
</mapper>