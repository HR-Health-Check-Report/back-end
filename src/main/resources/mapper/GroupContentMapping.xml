<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xxxx.hrhealthcheck.dao.GroupGradingContentDao" >
    <select id="getContent" resultType="com.xxxx.hrhealthcheck.bean.GroupGradingContent">
        select content from groupgrading
        where type  = #{group} and score = #{score}
    </select>
</mapper>